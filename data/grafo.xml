<?xml version="1.0" encoding="UTF-8"?>
<!--  An excerpt of an egocentric social network  -->
<graphml xmlns="http://graphml.graphdrawing.org/xmlns">
	<graph edgedefault="undirected">

		<!-- data schema -->
		<key id="name" for="node" attr.name="name" attr.type="string"/>
		<key id="gender" for="node" attr.name="gender" attr.type="string"/>

		<!-- nodes -->  
		<node id="1">
			<data key="name">Bruce</data>
			<data key="gender">M</data>
		</node>
		<node id="101">
			<data key="name">Ozzy</data>
			<data key="gender">F</data>
		</node>
		<node id="201">
			<data key="name">Eddie</data>
			<data key="gender">M</data>
		</node>
		<node id="5">
			<data key="name">Iron Maiden</data>
			<data key="gender">M</data>
		</node>
		<node id="105">
			<data key="name">Black Sabath</data>
			<data key="gender">M</data>
		</node>
		<node id="205">
			<data key="name">Pearl Jam</data>
			<data key="gender">M</data>
		</node>
		<edge source="1" target="5"></edge>
			
		<edge source="1" target="105"></edge>
			
		<edge source="1" target="205"></edge>
			
		<edge source="101" target="5"></edge>
			
		<edge source="101" target="105"></edge>
			
		<edge source="101" target="205"></edge>
			
		<edge source="201" target="5"></edge>
			
		<edge source="201" target="105"></edge>
			
		<edge source="201" target="205"></edge>
			

	</graph>
</graphml>